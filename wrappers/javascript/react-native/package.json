    "example": "yarn --cwd example",
    "tscodegen": "react-native-tscodegen ./react-native-tscodegen.json"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "react-native-tscodegen-types": "0.67.3"
  },
  "devDependencies": {
    "react-native-tscodegen": "0.68.4",
    "@types/react": "17.0.21",
    "@types/react-native": "0.66.15",
    "react": "17.0.2",
    "react-native": "0.66.4",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
