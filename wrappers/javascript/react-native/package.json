{
  "name": "react-native-indy-credx",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "description": "React Native wrapper for Indy Credx",
  "react-native": "src/index",
  "source": "src/index",
  "main": "build/index",
  "types": "build/index",
  "homepage": "https://github.com/hyperledger/indy-shared-rs/tree/main/wrappers/javascript/react-native",
  "author": "Hyperledger (https://github.com/hyperledger)",
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperledger/indy-shared-rs",
    "directory": "wrappers/javascript/react-native"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src",
    "build",
    "lib",
    "android",
    "ios",
    "cpp",
    "indy-credx-react-native.podspec",
